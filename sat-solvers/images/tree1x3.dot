digraph {
    at [style="invisible"]
    cf [style="invisible",width=1]
    bt [style="invisible"]
    a  [xlabel="1"]
    c  [xlabel="2"]
    b  [style="invisible"]
    a -> at [color="green",style="dashed"]
    a -> c  [color="red",label="¬a,d"]
    c -> b  [color="green",label="c,¬f,j",weight=2]
    c -> cf [color="red",style="dashed"]
    b -> cf [style="invisible",arrowhead=none]
    {rank=same; cf b}
    overlap=false
    splines=true
    bgcolor=transparent
}
